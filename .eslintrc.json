{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "plugins": [
        "react"
    ],
    "rules": {
        "func-names":"off",
        "jsx-quotes": ["error", "prefer-single"],
        "no-console":"off",
        "linebreak-style": "off"
    }
}
